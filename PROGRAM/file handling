#creating a text file
file = open("myfile.txt", "w") #opening the file
AQ = ["This is Python" ]
file.write("Hello There \n")
file.writelines(AQ)
#closing the file
file.close()
#opening the existing file
file = open("myfile.txt", "r+")
print("The output is ")
#reading the file
print(file.read())
print()
file.seek(0)
print("The output is ")
print(file.readline())
print()
file.seek(0)
print("The output(read(12)) is ")
print(file.read(12))
print()
file.seek(0)
print("The output(readline(8)) is ")
print(file.readline(8))
file.seek(0)
print("The output(readline) is")
print(file.readlines())
print()
file.close()
